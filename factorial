# Factorial number

package myjava;

import java.util.Scanner;

public class Factorial {

	public static void main(String[] args) {
		
		
		// TODO Auto-generated method stub
		
		int n,i,fact = 1;
		
		
        System.out.println("Enter number: ");
		
		Scanner sc = new Scanner(System.in);
		
		n = sc.nextInt();
		
		for(i = n ; i >= 1 ; i--)
		{
			fact = fact * i;
		}
		
		System.out.println(fact);

	}

}
